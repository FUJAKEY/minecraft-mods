{
  "multipart": [
    {"apply": {"model": "energymod:block/energy_wire_center"}},
    {"when": {"north": true}, "apply": {"model": "energymod:block/energy_wire_side"}},
    {"when": {"east": true}, "apply": {"model": "energymod:block/energy_wire_side", "y": 90}},
    {"when": {"south": true}, "apply": {"model": "energymod:block/energy_wire_side", "y": 180}},
    {"when": {"west": true}, "apply": {"model": "energymod:block/energy_wire_side", "y": 270}},
    {"when": {"up": true}, "apply": {"model": "energymod:block/energy_wire_side", "x": -90}},
    {"when": {"down": true}, "apply": {"model": "energymod:block/energy_wire_side", "x": 90}}
  ]
}
